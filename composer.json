{
    "name": "trahey/powernap-bundle",
    "type": "symfony-bundle",
    "description": "ReST server with annotation-based declarative config, built on top of Doctrine.",
    "keywords": ["rest", "json", "xml", "doctrine"],
    "homepage": "http://www.christrahey.com/bundles/TraheyPowerNapBundle",
    "license": "TBD",
    "authors": [
        {
            "name": "Christopher Trahey",
            "email": "christrahey@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jms/serializer": ">=0.11",
        "symfony/framework-bundle": "~2.1",
        "sensio/framework-extra-bundle": "^3.0.2",
        "friendsofsymfony/rest-bundle": "^2.0",
        "gedmo/doctrine-extensions":      "dev-master"
    },
    "require-dev": {
        "symfony/yaml": "*",
        "symfony/class-loader": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "symfony/process": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-0": { "Trahey\\PowerNapBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Trahey/PowerNapBundle",
    "extra": {
    }
}